<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:height="32sp"
        android:width="32sp"
        android:top="-2sp">
        <shape android:shape="oval" android:padding="10sp">
            <solid android:color="#66FFFFFF" />
            <corners
                android:bottomRightRadius="5sp"
                android:bottomLeftRadius="5sp"
                android:topLeftRadius="5sp"
                android:topRightRadius="5sp"/>
        </shape>
    </item>
    <item
        android:height="32sp"
        android:width="32sp"
        android:top="3sp"
        android:left="4sp">
        <shape android:shape="oval" android:padding="10sp">
            <solid android:color="#1F000000" />
            <corners
                android:bottomRightRadius="5sp"
                android:bottomLeftRadius="5sp"
                android:topLeftRadius="5sp"
                android:topRightRadius="5sp"/>
        </shape>
    </item>
    <item
        android:height="32dp"
        android:width="32dp"
        android:left="2sp"
        >
        <shape android:shape="oval" android:padding="0dp">
            <solid android:color="#ffffff" />
            <corners
                android:bottomRightRadius="5dp"
                android:bottomLeftRadius="5dp"
                android:topLeftRadius="5dp"
                android:topRightRadius="5dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_add_teal_36"
        android:height="32dp"
        android:width="32dp"
        android:top="6sp"
        android:left="7sp"/>
</layer-list>