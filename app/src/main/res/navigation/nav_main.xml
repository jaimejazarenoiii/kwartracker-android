<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/nav_home">
    <fragment
        android:id="@+id/nav_home"
        android:name="com.kwartracker.android.home.ui.HomeFragment"
        android:label="Home" />

    <fragment
        android:id="@+id/transactions_details_fragment"
        android:name="com.kwartracker.android.transactions.ui.details.TransactionDetailsFragment"
        android:label="Transaction Details"
        tools:layout="@layout/fragment_transaction_details" />

    <fragment
        android:id="@+id/transaction_lists_fragment"
        android:name="com.kwartracker.android.transactions.ui.list.TransactionsListFragment"
        android:label="Transaction Details"
        tools:layout="@layout/fragment_transactions_list" />

    <fragment
        android:id="@+id/transaction_add_fragment"
        android:name="com.kwartracker.android.transactions.ui.add.TransactionAddFragment"
        android:label="Transaction Add"
        tools:layout="@layout/fragment_transaction_add" />

    <fragment
        android:id="@+id/walletsFragment"
        android:name="com.kwartracker.android.wallet.ui.MyWalletsFragment"
        android:label="Login"
        tools:layout="@layout/fragment_wallets" />
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.kwartracker.android.profile.ui.MyProfileFragment"
        android:label="Login"
        tools:layout="@layout/fragment_my_profile" />
